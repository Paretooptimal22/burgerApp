<div>
  <form>
    <p>
      <label for="burger">Burger: </label>
      <input type="text" name="burger" id="burger">
    </p>
    <p>
      <button id="addBurger">Add Burger</button>
    </p>
  </form>
  {{#each burgers}}
    <div>
      <p>
        Burger: {{name}}
      </p>
      <p>
        Eaten:
        <span>
          {{#if eaten}}
          Yes
          {{else}}
          No
          {{/if}}
        </span>
      </p>
    </div>
  {{/each}}
  <div class="container">
    <div class="row">
      <form class="col s12">
        <div class="row">
          <div class="col s8 input-field">
            <input type="text" class="validate" name="burger" id="burger">
            <label for="burger">Burger</label>
          </div>
          <div class="col s4 input-field">
            <button class="btn-large btn-floating waves-effect waves-light">
              <i class="material-icons">
                
              </i>
            </button>
          </div>
        </div>
      </form>
    </div>
  </div>
</div>