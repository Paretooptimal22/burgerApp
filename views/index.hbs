<div class="container">
  <div class="row">
    <form class="col s11">
      <div class="row">
        <div class="col s10 input-field">
          <input type="text" class="validate" name="burger" id="burger">
          <label for="burger">Burger</label>
        </div>
        <div class="col s1 input-field">
          <button class="btn-large btn-floating waves-effect waves-light">
            <i class="material-icons">
              add
            </i>
          </button>
        </div>
      </div>
    </form>
  </div>
  <div class="row">
    <div id="unEaten" class="col s4">
      <h5 class="center-align">Un-Eaten Burgers</h5>
      {{#each burgers}}
      {{#unless eaten}}
        <div class="row card-panel">
          <p class="center-align"><b>{{name}}</b></p>
        </div>
      {{/unless}}
      {{/each}}
    </div>
    <div class="col s4"></div>
    <div id="eaten" class="col s4">
      <h5 class="center-align">Eaten Burgers</h5>
      {{#each burgers}}
      {{#if eaten}}
        <div class="row card-panel">
          <p class="center-align"><b>{{name}}</b></p>
        </div>
      {{/if}}
      {{/each}}
    </div>
  </div>
</div>